{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "DATABASE_URL": "@database_url"
    },
    "installCommand": "npm install",
    "buildCommand": "npm run generate && npm run deploy",
    "outputDirectory": ".",
    "framework": null
  }
  